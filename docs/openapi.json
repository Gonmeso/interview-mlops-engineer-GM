{
    "openapi": "3.0.2",
    "info": {
        "title": "MLOps Engineer Predictions API",
        "description": "\nAPI to make predictions using a Tensorflow model! ðŸš€\n\n## Predict\n\nYou can perform predictions!\n\n",
        "license": {
            "name": "MIT License"
        },
        "version": "0.0.1"
    },
    "paths": {
        "/status": {
            "get": {
                "summary": "Status",
                "description": "Healthcheck for the API\n\n",
                "operationId": "status_status_get",
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        },
        "/predict": {
            "post": {
                "summary": "Predict Model",
                "description": "Performs the prediction with the loaded model usingthe request data\n\n",
                "operationId": "predict_model_predict_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/InputData"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseData"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "HTTPValidationError": {
                "title": "HTTPValidationError",
                "type": "object",
                "properties": {
                    "detail": {
                        "title": "Detail",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        }
                    }
                }
            },
            "InputData": {
                "title": "InputData",
                "required": [
                    "data"
                ],
                "type": "object",
                "properties": {
                    "data": {
                        "title": "Data",
                        "type": "array",
                        "items": {
                            "type": "integer"
                        }
                    }
                },
                "description": "Represents the expected body to the `predict` endpoint.",
                "data": [
                    10
                ]
            },
            "ResponseData": {
                "title": "ResponseData",
                "required": [
                    "prediction"
                ],
                "type": "object",
                "properties": {
                    "prediction": {
                        "title": "Prediction",
                        "type": "array",
                        "items": {
                            "type": "number"
                        }
                    }
                },
                "description": "Represents the response to the `predict` endpoint",
                "prediction": [
                    30.102
                ]
            },
            "ValidationError": {
                "title": "ValidationError",
                "required": [
                    "loc",
                    "msg",
                    "type"
                ],
                "type": "object",
                "properties": {
                    "loc": {
                        "title": "Location",
                        "type": "array",
                        "items": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "integer"
                                }
                            ]
                        }
                    },
                    "msg": {
                        "title": "Message",
                        "type": "string"
                    },
                    "type": {
                        "title": "Error Type",
                        "type": "string"
                    }
                }
            }
        }
    }
}
